{
  "name": "Hello-AR",
  "version": "1.0.0",
  "description": "Hello AR is a simple application to test WebXR Device API.",
  "main": "index.html",
  "repository": {
    "type": "git",
    "url": "https://gitlab.devtools.intel.com/iags-ssp-web-platform-team/webxr-samples"
  },
  "scripts": {
    "start": "es-dev-server --node-resolve --app-index index.html --open --watch --babel",
    "start:build": "es-dev-server dist/ --open",
    "build": "rimraf dist && rollup -c rollup.config.js"
  },
  "keywords": [
    "XR",
    "AR",
    "WebXR"
  ],
  "author": "Alexis Menard",
  "license": "BSD-2-Clause",
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@open-wc/building-rollup": "^0.18.1",
    "@webcomponents/webcomponentsjs": "^2.4.0",
    "es-dev-server": "^1.45.1",
    "rollup": "^2.0.6",
    "rollup-plugin-babel": "^4.4.0",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "rollup-plugin-copy-glob": "^0.3.1",
    "rollup-plugin-copy": "^3.3.0"
  },
  "dependencies": {
    "@material/mwc-button": "^0.13.0",
    "@material/mwc-icon": "^0.13.0",
    "@material/mwc-icon-button": "^0.13.0",
    "@material/mwc-linear-progress": "^0.13.0",
    "@material/mwc-snackbar": "^0.13.0",
    "lit-element": "^2.2.1",
    "three": "^0.114.0",
    "workbox-build": "^5.0.0",
    "workbox-sw": "^5.0.0"
  }
}
